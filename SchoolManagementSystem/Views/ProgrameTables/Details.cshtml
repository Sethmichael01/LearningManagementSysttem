@model DataAccess.ProgrameTable

@{
    ViewBag.Title = "Programe";
}

<div>
    <h4>Programe</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Programe Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            Start Date
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StartDate)
        </dd>

        <dt>
            Status
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsActive)
        </dd>

        <dt>
            User
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserTable.FullName)
        </dd>

    </dl>
</div>
<p class="btn-group">
    @Html.ActionLink("Edit", "Edit", new { id = Model.ProgrameID }, new { @class = "btn btn-info"}) 
    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary"})
</p>
